@model List<String>
@{
    ViewData["Title"] = "Index";
}

<div class="p-4">
    <h3>Configuration from AppSettings</h3>
    <ul>
        @foreach(var message in @Model)
        {
            <li>@message</li>
        }
    </ul>
</div>



